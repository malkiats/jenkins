# Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage ('test') {
            steps {
                echo "Hello World"
            }
        }
        stage ('build') {
            steps {
                echo "Hello World"
            }
        }
        stage ('deploy') {
            steps {
                echo "Hello World"
            }
        }
        
    }
}


# Jenkinsfile (Scripted Pipeline)
node {
    stage('build') {
        //
    }
    stage('Test') {
        //
    }
    stage('Deploy') {
        //
    }
}
